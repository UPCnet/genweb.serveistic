<tal:define tal:define="notificacions view/notificacions">
<tal:repeat tal:repeat="notificacio notificacions">
<div class="item">
  <a tal:attributes="href notificacio/url">
    <span tal:attributes="class python: view.get_awesome_icon_class(notificacio)"></span>
    <span class="data" tal:content="notificacio/data"></span>
    <span class="titol" tal:content="notificacio/titol"></span>
  </a>
</div>
</tal:repeat>
</tal:define>